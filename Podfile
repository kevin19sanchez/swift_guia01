# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'


target 'Guia01' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Guia01
    #Libreria Networking
    pod 'Alamofire', '~> 4.5'
    #Libreria Serializacion Json
    pod 'SwiftyJSON'
    #Libreria Descarga Imagenes
    pod 'SDWebImage', '~> 4.0'

  target 'Guia01Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Guia01UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
